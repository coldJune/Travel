<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
    <struts>
    	<constant name="struts.enable.DynamicMethodInvocation" value="false"></constant>
    	<constant name="struts.devMode" value="true"></constant>
    	<package name="default" namespace="/" extends="struts-default">
    		 <action name="emp-*" class="employeeAction" method="{1}">
    			<result name="list">emp/emp-list.jsp</result>
    		</action>
    	</package>
    	<package name="user" namespace="/user" extends="struts-default">
    		<action name="logiin" class="userAction" method="login">
    			<result name="login"></result>
    		</action>
    	</package>
    	<package name="albumPhoto" extends="struts-default">
    		<action name="uploadPhotos" class="com.competition.db.action.AlbumPhotoAction" method="uploadPhotos">
    			<param name="savePath">/</param>
    		</action>
    	</package>
    </struts>